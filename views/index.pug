doctype html 
head 
    title Bokhandel
    link(rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css'
    integrity='sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u' crossorigin='anonymous')
        
body
    h1 LäsvärdaBöcker.se
    table(class="table")
        thead
            tr
                th(scope="col") ISBN
                th(scope="col") Titel
                th(scope='col') Författare
                th(scope='col') Pris
        tbody
            each bok in Böcker
                tr
                    th
                        a(href ='book/' + bok.ISBN13 scope="row") #{bok.ISBN13}
                    td
                        a(href ='book/' + bok.ISBN13) #{bok.Titel}
                    td
                        a(href ='book/' + bok.ISBN13) #{bok.Förf}
                    td
                        a(href ='book/' + bok.ISBN13) #{bok.pris}                